{
    "name": "clabonte/BehatProfiling",
    "type": "library",
    "description": "Behat context used to profile automated test execution in order to use them in a load/stress testing mode and export the result in a jMeter-compatible format",
    "keywords": ["Behat", "load testing", "stress testing", "jMeter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Labonte"
        }
    ],
    "require": {
        "behat/behat": "^3.3",
        "myclabs/php-enum": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0",
        "squizlabs/php_codesniffer": "^2.9 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "BehatProfiling\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BehatProfiling\\": "tests/"
        }
    }
}
